{
    "name": "ghivarra/aether",
    "description": "Aether PHP Framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aether\\": "core/",
            "App\\": "app/",
            "Config\\": "config/",
            "Test\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Ghivarra Senandika Rushdie",
            "email": "gsenandika@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "fakerphp/faker": "2.0.x-dev",
        "phpunit/phpunit": "^12.3@dev",
        "kint-php/kint": "dev-master"
    },
    "require": {
        "laminas/laminas-escaper": "2.18.x-dev",
        "predis/predis": "dev-main"
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
